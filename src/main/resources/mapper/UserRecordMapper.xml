<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codeloom.mapper.UserRecordMapper">

    <resultMap id="BaseResultMap" type="com.codeloom.domain.UserRecord">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <id property="uid" column="uid" jdbcType="VARCHAR"/>
            <result property="rating" column="rating" jdbcType="INTEGER"/>
            <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
            <result property="gmtModified" column="gmt_modified" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,uid,rating,
        gmt_create,gmt_modified
    </sql>
</mapper>
