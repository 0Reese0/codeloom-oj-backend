<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codeloom.mapper.LanguageMapper">

    <resultMap id="BaseResultMap" type="com.codeloom.domain.Language">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="contentType" column="content_type" jdbcType="VARCHAR"/>
            <result property="description" column="description" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="compileCommand" column="compile_command" jdbcType="VARCHAR"/>
            <result property="template" column="template" jdbcType="VARCHAR"/>
            <result property="codeTemplate" column="code_template" jdbcType="VARCHAR"/>
            <result property="isSpj" column="is_spj" jdbcType="TINYINT"/>
            <result property="oj" column="oj" jdbcType="VARCHAR"/>
            <result property="seq" column="seq" jdbcType="INTEGER"/>
            <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
            <result property="gmtModified" column="gmt_modified" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,content_type,description,
        name,compile_command,template,
        code_template,is_spj,oj,
        seq,gmt_create,gmt_modified
    </sql>
</mapper>
